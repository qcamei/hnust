SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`simplecontent`,`link`,`author`,`source`,`time`,`type`) VALUES ('ef95937d9b74b61bd53fe47d5617e48d','科大政发〔2016〕90号',' <div class=\"panel\">
