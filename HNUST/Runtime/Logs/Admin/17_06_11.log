SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`link`,`author`,`source`,`time`,`type`,`imgurl`,`newsimgs`) VALUES ('567a870b64c7c223741c4d3facce094c','我校开展教学院本科教学审核评估自评工作（二）','&lt;p&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;5月9日，我校物理与电子科学学院、商学院、体育学院、资源环境与安全工程学院分别展开了教学院本科教学审核评估自评工作。评估小组组长、党委副书记朱川曲，评估小组组长、副校长陈安华、胡石其、施式亮及评估专家组其他成员就学院基本情况、定位与目标、师资队伍、教学资源、培养过程、学生发展、质量保证及办学特色等方面展开评估。&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; 专家见面会上，四个学院分别作评估工作汇报。物理与电子科学学院负责人详细汇报了学院的“物理学专业校企合作创新创业教育基地”、第二课堂教学、物理学理论教学及实验教学三个特色项目。体育学院负责人重点提到该院发挥专业优势，开展“教学、训练、展演汇报”一体化活动的办学特色。“面向社会，面向现代化，注重实验与教学相结合，切实将专业知识落到实处”商学院负责人详细介绍了学院的实践办学特色及人才培养理念。资源环境与安全工程学院负责人表示，学院致力建立质量保障体系，稳固本科教学中心地位，促进教学研讨制度化，推进常规教学质量监控常态化，建立多元化评教体系。&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; 汇报会结束后，各专家组组长对各学院本科教学工作进行点评，并提出相关要求。&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;朱川曲在专家意见反馈会上对物理与电子科学学院开展的教学工作以及自评工作上表示肯定。同时，他就该院在学院定位以及人才培养目标上提出建议：“学院开展的特色项目要加强整合，并以此作为独属于本学院的特色项目着重发展。”&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; 对商学院的教学及自评工作，陈安华给予了高度评价：“该院建立覆盖教学全过程的监控体系，前期、中期、后期监控三个环节，环环相扣，质量稳步提高，学风良好。”同时，他指出，商学院应当加强教研与教学的互动，以科研促教学。&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; 胡石其在反馈会上表示：“体育学院的办学定位十分明确，近几年来引进的人才质量逐步提高，整体教学资源有明显优化。”他同时建议体育学院应增强开放办学的力度，充分利用学校的办学资源，强化办学特色。&lt;/span&gt;&lt;br/&gt;&lt;br/&gt;&lt;span style=&quot;font-family: 宋体; font-size: 14px; text-indent: 28px;&quot;&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp; 施式亮在专家意见反馈会上肯定了资源环境与安全工程学院在本科教学工作中取得的成效。对如何进一步提升本科教学质量，他指出，学院应该进一步凸显办学特色，丰富课程资源。&lt;/span&gt;&lt;/p&gt;','home/file/getfile?type=4&id=567a870b64c7c223741c4d3facce094c','文/德吉卓玛 康欣可 翁素丽 邓晓亿 图/孙继 何鑫 柳姝萌','大学生通讯社','2017-05-09','4','/hnust/Public/img/2017-06-11/593d2e9f516e4.jpg','/hnust/Public/img/2017-06-11/593d2e9f51ff4.jpg,/hnust/Public/img/2017-06-11/593d2e9f53012.jpg,/hnust/Public/img/2017-06-11/593d2e9f53871.jpg,/hnust/Public/img/2017-06-11/593d2e9f5436b.jpg,/hnust/Public/img/2017-06-11/593d2e9f54ea0.jpg') [ RunTime:0.0217s ]
