SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`simplecontent`,`link`,`author`,`source`,`time`,`type`) VALUES ('43d039c5a93777db766306c6c2fbadbc','关于公布我省2016年普通高等学校
本科教学工作审核评估计划的通知',' <div class=\"panel\">
    <div class=\"panel-heading\">
      <h3 class=\"news-title\">关于公布我省2016年普通高等学校
本科教学工作审核评估计划的通知</h3>
      <div class=\"news-other\">
        作者：<span id=\"author\"></span>&nbsp;&nbsp;&nbsp;&nbsp;来源：<span id=\"Source\">教育厅</span>&nbsp;&nbsp;&nbsp;&nbsp;上传时间：<span id=\"Time\">2016-04-15</span>
      </div>
      <div class=\"split\"></div>
