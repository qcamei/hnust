SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`simplecontent`,`link`,`author`,`source`,`time`,`type`,`imgurl`) VALUES ('ca0288ad8df1db9a1d7e7d1789517067','2016-2017学年第一学期“教学礼拜”主题活动协调会召开',' <div class=\"panel\">
