SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`simplecontent`,`link`,`author`,`source`,`time`,`type`) VALUES ('d20e152a4e7cc99bdf92ebae832393a7','关于做好湖南科技大学2016-2017学年第一学期“教学礼拜”主题活动教学巡查工作的通知 ',' <div class=\"panel\">
    <div class=\"panel-heading\">
      <h3 class=\"news-title\">关于做好湖南科技大学2016-2017学年第一学期“教学礼拜”主题活动教学巡查工作的通知 </h3>
      <div class=\"news-other\">
        作者：<span id=\"author\">蒋云霞</span>&nbsp;&nbsp;&nbsp;&nbsp;来源：<span id=\"Source\">质量监控与评估办公室 </span>&nbsp;&nbsp;&nbsp;&nbsp;上传时间：<span id=\"Time\">2016-10-26</span>
