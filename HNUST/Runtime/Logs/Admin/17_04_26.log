SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`link`,`author`,`source`,`time`,`type`) VALUES ('972f0af79ba40a69e2c45da7bd63740d','关于印发《湖南科技大学教学院教学档案
指导性目录》的通知','&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size:21px;font-family:仿宋_GB2312&quot;&gt;科大政发〔2017〕2号&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align:center;line-height:37px&quot;&gt;&lt;span style=&quot;font-family: 方正小标宋_GBK; font-size: 24px;&quot;&gt;关于印发《湖南科技大学教学院教学档案&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align:center;line-height:37px&quot;&gt;&lt;span style=&quot;font-family: 方正小标宋_GBK; font-size: 24px;&quot;&gt;指导性目录》的通知&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align:center;line-height:37px&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size: 27px;font-family: 方正小标宋_GBK&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;line-height: 37px&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;&lt;span style=&quot;font-family: 仿宋_GB2312;&quot;&gt;各教学单位：&lt;/span&gt;&lt;span style=&quot;font-family: 仿宋_GB2312;&quot;&gt;&lt;a&gt;&lt;/a&gt;&lt;/span&gt;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-family: 仿宋_GB2312; font-size: 18px;&quot;&gt;教学档案是日常教学运行、教学管理和学生学习过程中形成的原&lt;span style=&quot;font-size: 18px; font-family: 仿宋_GB2312; letter-spacing: -1px;&quot;&gt;始记录、原始材料、统计数据和总结材料，&lt;/span&gt;是教学活动和教学评估不可缺少的重要依据和主要资料。为做好学校本科教学审核评估与建设相关工作，特制定《湖南科技大学教学院教学档案指导性目录》，现予以印发，请各单位组织认真学习并遵照执行。&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-family: 仿宋_GB2312; font-size: 20px;&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent: 378px; line-height: 37px; text-align: right;&quot;&gt;&lt;span style=&quot;font-family: 仿宋_GB2312&quot;&gt;湖南科技大学 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent: 368px; line-height: 37px; text-align: right;&quot;&gt;&lt;span style=&quot;font-family: 仿宋_GB2312&quot;&gt;2017年1月5日 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p&gt;&lt;span style=&quot;font-family: 仿宋_GB2312; font-size: 20px;&quot;&gt;&lt;/span&gt;&lt;/p&gt;&lt;hr/&gt;&lt;p&gt;附件：&lt;/p&gt;&lt;p style=&quot;lin