SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`link`,`author`,`source`,`time`,`type`) VALUES ('8b796a6351fcae33d761fe9c442516f3','关于成立本科教学评估与建设办公室的通知','&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size:21px;font-family:仿宋_GB2312&quot;&gt;科大政发〔2017〕14号&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align:center;line-height:32px&quot;&gt;&lt;span style=&quot;font-size:27px;font-family:方正小标宋_GBK&quot;&gt;关于成立本科教学评估与建设办公室的通知&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align:center;line-height:32px&quot;&gt;&lt;strong&gt;&lt;span style=&quot;font-size:24px&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/strong&gt;&lt;/p&gt;&lt;p style=&quot;line-height:32px&quot;&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;校属各单位：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:43px;line-height:32px&quot;&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;为确保高质量、高标准、高要求完成本科教学审核评估与建设各项任务，根据工作需要，经校党委研究决定，成立本科教学审核评估与建设办公室，由以下人员组成：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:43px;line-height:32px&quot;&gt;&lt;strong&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;主&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;;font-family:等线&quot;&gt;&amp;nbsp; &amp;nbsp;&amp;nbsp;&lt;/span&gt;&lt;/strong&gt;&lt;strong&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;任：&lt;/span&gt;&lt;/strong&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;杨&lt;/span&gt;&lt;span style=&quot;;font-family:等线&quot;&gt;&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;清&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:38px;line-height:32px&quot;&gt;&lt;strong&gt;&lt;span style=&quot;;font-family:仿宋_GB2312;letter-spacing:-1px&quot;&gt;常务副主任：&lt;/span&gt;&lt;/strong&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;何正良&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:43px;line-height:32px&quot;&gt;&lt;strong&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;副&lt;/span&gt;&lt;/strong&gt;&lt