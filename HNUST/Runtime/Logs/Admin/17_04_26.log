SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`link`,`author`,`source`,`time`,`type`) VALUES ('3f6c5878973e9064d2a018fa4464561c','湖南科技大学关于调整审核评估时间的请示','&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size:21px;font-family:仿宋_GB2312&quot;&gt;科大政字〔2016〕19号&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;line-height:37px&quot;&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;省教育厅：&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:40px;line-height:37px&quot;&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;按照省教育厅《关于启动湖南省普通高等学校本科教学工作审核评估的通知》（湘教通〔2016〕102号）要求，学校高度重视，对审核评估工作进行了认真研究。根据教育部和湖南省深化教育领域综合改革的总体要求，今年学校全面启动了综合改革，将完成学校院（系）设置及学科专业布局调整。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:40px;line-height:37px&quot;&gt;&lt;span style=&quot;;font-family:仿宋_GB2312&quot;&gt;为更好地迎接和做好审核评估工作，我校申请将审核评估的时间延期至2017年11月下旬。&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: right;&quot;&gt;&lt;span style=&quot;font-family: 仿宋_GB2312; font-size: 16px;&quot;&gt;湖南科技大学 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: right;&quot;&gt;&lt;span style=&quot;font-family: 仿宋_GB2312; font-size: 16px;&quot;&gt;2016年4月11日 &amp;nbsp; &amp;nbsp; &amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;/p&gt;','home/file/getfile?type=0&id=3f6c5878973e9064d2a018fa4464561c','','湖南科技大学','2016-04-11','0') [ RunTime:0.0020s ]
