SQL: INSERT INTO `hnust_supervisor` (`id`,`title`,`content`,`link`,`author`,`source`,`time`,`type`) VALUES ('944bf7e8776968ee0918f56115d7f7d6','教学督导简报第27期','&lt;p style=&quot;text-align:center&quot;&gt;&lt;span style=&quot;font-size:56px;letter-spacing:-1px&quot;&gt;&amp;nbsp;&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align:center&quot;&gt;&lt;span style=&quot;font-size:56px;font-family:黑体;color:red;letter-spacing:-1px&quot;&gt;教&lt;/span&gt; &lt;span style=&quot;font-size: 56px;font-family:黑体;color:red;letter-spacing:-1px&quot;&gt;学&lt;/span&gt; &lt;span style=&quot;font-size:56px;font-family:黑体;color:red;letter-spacing:-1px&quot;&gt;督&lt;/span&gt; &lt;span style=&quot;font-size:56px;font-family:黑体;color:red;letter-spacing:-1px&quot;&gt;导&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-indent:28px;line-height:32px&quot;&gt;&amp;nbsp;&lt;/p&gt;&lt;p style=&quot;text-indent:37px;line-height:32px&quot;&gt;&lt;span style=&quot;font-size:19px&quot;&gt;&amp;nbsp; &amp;nbsp;&lt;/span&gt;&lt;span style=&quot;font-size:19px;font-family:楷体_GB2312&quot;&gt;第&lt;/span&gt;&lt;span style=&quot;font-size:19px&quot;&gt;&amp;nbsp; 27&amp;nbsp; &lt;/span&gt;&lt;span style=&quot;font-size:19px;font-family:楷体_GB2312&quot;&gt;期&lt;/span&gt;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp; &amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&amp;nbsp;&lt;span style=&quot;font-size: 19px&quot;&gt;&amp;nbsp;2015&lt;/span&gt;&lt;span style=&quot;font-size:19px;font-family:楷体_GB2312&quot;&gt;年&lt;/span&gt;&lt;span style=&quot;font-size:19px&quot;&gt;5&lt;/span&gt;&lt;span style=&quot;font-size:19px;font-family:楷体_GB2312&quot;&gt;月&lt;/span&gt;&lt;span style=&quot;font-size:19px&quot;&gt;15&lt;/span&gt;&lt;span style=&quot;font-size:19px;font-family:楷体_GB2312&quot;&gt;日&lt;/span&gt;&lt;/p&gt;&lt;table&gt;&